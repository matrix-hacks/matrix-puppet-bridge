{
  "name": "matrix-puppet-bridge",
  "version": "1.4.1",
  "description": "facilitates writing a certain kind of matrix bridge",
  "main": "index.js",
  "scripts": {
    "test": "echo \"no tests at this time\"; exit 1"
  },
  "keywords": [
    "matrix"
  ],
  "contributors": [
    "Keyvan Fatehi",
    "Andrew Johnson"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewJDR/matrix-puppet-bridge.git"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "debug": "^2.6.0",
    "matrix-appservice-bridge": "^1.3.3",
    "matrix-js-sdk": "^0.7.2",
    "read": "^1.0.7"
  },
  "devDependencies": {
    "eslint": "^3.12.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": 0,
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
